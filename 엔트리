    var indexPath = '/#!/?showLogin=true';
    var isClick;
    var isLogin = true;
    if(!window.user) {
        isLogin = false;
        positiveButtonText = Lang.Menus.login;
    }

    if('' && '' == 'emailAuth') {
        showEmailAuthAlert(Lang.Users.verification_complete);
    } else if ('' && '' == 'invalidEmailAuthRequest') {
        showEmailAuthAlert(Lang.Users.email_address_invalid_for_idpw);
    }

    function showEmailAuthAlert(content) {
        entrylms.alert({ 
            outerHTML : '<div style="font-size:16px;margin-bottom:20px">' + content + '</div><div style="font-size:16px">' + '' + '</div>',
        }, null, {
            positiveButtonText: positiveButtonText,
        }).on('click', function (type) {
            isClick = true;
            if(type === 'ok' && !isLogin) {
                window.location.href = indexPath;
            } else {
                window.location.href = defaultPath;
            }
        }).on('hide', function() {
            if(!isClick) {
                window.location.href = defaultPath;
            }
        });
    }
